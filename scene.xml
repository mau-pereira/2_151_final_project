    <geom name="z_axis_cylinder" type="cylinder" fromto="0 0 0 0 0 0.5" size="0.01" rgba="0 0 1 1" contype="0" conaffinity="0" group="0"/>
    <geom name="floor" size="0 0 0.05" type="plane" material="groundplane" contype="0" conaffinity="0"/>
